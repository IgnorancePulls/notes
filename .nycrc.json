{
  "all": true,
  "include": [
    "src/**/*.{js,ts,tsx}"
  ],
  "exclude": [
    "src/test/**",
    "**/*.d.ts",
    "**/*.config.*",
    "**/*.test.{ts,tsx}",
    "**/mockData/**",
    "src/main.tsx"
  ],
  "reporter": [
    "html",
    "lcov",
    "text",
    "json-summary"
  ],
  "report-dir": "./coverage/combined",
  "temp-dir": "./coverage/.nyc_output",
  "sourceMap": true,
  "instrument": false,
  "cache": false
}
